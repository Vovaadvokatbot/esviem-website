{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://esviem.kiev.ua/:path*",
      "has": [{ "type": "host", "value": "www.esviem.kiev.ua" }],
      "permanent": true
    },
    {
      "source": "/:path*",
      "destination": "https://esviem.kiev.ua/:path*",
      "has": [{ "type": "header", "key": "x-forwarded-proto", "value": "http" }],
      "permanent": true
    }
  ]
}